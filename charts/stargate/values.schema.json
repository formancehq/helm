{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
        "affinity": {
            "type": "object"
        },
        "autoscaling": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "maxReplicas": {
                    "type": "integer"
                },
                "minReplicas": {
                    "type": "integer"
                },
                "targetCPUUtilizationPercentage": {
                    "type": "integer"
                }
            }
        },
        "aws": {
            "type": "object",
            "properties": {
                "targetGroups": {
                    "type": "object",
                    "properties": {
                        "grpc": {
                            "type": "object",
                            "properties": {
                                "ipAddressType": {
                                    "type": "string"
                                },
                                "serviceRef": {
                                    "type": "object",
                                    "properties": {
                                        "name": {
                                            "type": "string"
                                        },
                                        "port": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "targetGroupARN": {
                                    "type": "string"
                                },
                                "targetType": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        },
        "config": {
            "type": "object",
            "properties": {
                "auth_issuer_url": {
                    "type": "string"
                },
                "publisher": {
                    "type": "object",
                    "properties": {
                        "clientID": {
                            "type": "string"
                        },
                        "topicMapping": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "fullnameOverride": {
            "type": "string"
        },
        "global": {
            "type": "object",
            "properties": {
                "aws": {
                    "type": "object",
                    "properties": {
                        "elb": {
                            "type": "boolean"
                        },
                        "iam": {
                            "type": "boolean"
                        }
                    }
                },
                "debug": {
                    "type": "boolean"
                },
                "licence": {
                    "type": "object",
                    "properties": {
                        "clusterID": {
                            "type": "string"
                        },
                        "existingSecret": {
                            "type": "string"
                        },
                        "issuer": {
                            "type": "string"
                        },
                        "secretKeys": {
                            "type": "object",
                            "properties": {
                                "token": {
                                    "type": "string"
                                }
                            }
                        },
                        "token": {
                            "type": "string"
                        }
                    }
                },
                "monitoring": {
                    "type": "object",
                    "properties": {
                        "batch": {
                            "type": "boolean"
                        },
                        "logs": {
                            "type": "object",
                            "properties": {
                                "enabled": {
                                    "type": "boolean"
                                },
                                "format": {
                                    "type": "string"
                                },
                                "level": {
                                    "type": "string"
                                }
                            }
                        },
                        "metrics": {
                            "type": "object",
                            "properties": {
                                "enabled": {
                                    "type": "boolean"
                                },
                                "endpoint": {
                                    "type": "string"
                                },
                                "exporter": {
                                    "type": "string"
                                },
                                "insecure": {
                                    "type": "boolean"
                                },
                                "mode": {
                                    "type": "string"
                                },
                                "port": {
                                    "type": "integer"
                                }
                            }
                        },
                        "traces": {
                            "type": "object",
                            "properties": {
                                "enabled": {
                                    "type": "boolean"
                                },
                                "endpoint": {
                                    "type": "string"
                                },
                                "exporter": {
                                    "type": "string"
                                },
                                "insecure": {
                                    "type": "boolean"
                                },
                                "mode": {
                                    "type": "string"
                                },
                                "port": {
                                    "type": "integer"
                                }
                            }
                        }
                    }
                },
                "nats": {
                    "type": "object",
                    "properties": {
                        "auth": {
                            "type": "object",
                            "properties": {
                                "existingSecret": {
                                    "type": "string"
                                },
                                "password": {
                                    "type": "null"
                                },
                                "secretKeys": {
                                    "type": "object",
                                    "properties": {
                                        "password": {
                                            "type": "string"
                                        },
                                        "username": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "user": {
                                    "type": "null"
                                }
                            }
                        },
                        "enabled": {
                            "type": "boolean"
                        },
                        "requestTimeout": {
                            "type": "string"
                        },
                        "url": {
                            "type": "string"
                        }
                    }
                },
                "postgresql": {
                    "type": "object",
                    "properties": {
                        "additionalArgs": {
                            "type": "string"
                        },
                        "auth": {
                            "type": "object",
                            "properties": {
                                "database": {
                                    "type": "string"
                                },
                                "existingSecret": {
                                    "type": "string"
                                },
                                "password": {
                                    "type": "string"
                                },
                                "postgresPassword": {
                                    "type": "string"
                                },
                                "secretKeys": {
                                    "type": "object",
                                    "properties": {
                                        "adminPasswordKey": {
                                            "type": "string"
                                        },
                                        "userPasswordKey": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "username": {
                                    "type": "string"
                                }
                            }
                        },
                        "host": {
                            "type": "string"
                        },
                        "service": {
                            "type": "object",
                            "properties": {
                                "ports": {
                                    "type": "object",
                                    "properties": {
                                        "postgresql": {
                                            "type": "integer"
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "serviceHost": {
                    "type": "string"
                }
            }
        },
        "image": {
            "type": "object",
            "properties": {
                "pullPolicy": {
                    "type": "string"
                },
                "repository": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "ingress": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "className": {
                    "type": "string"
                },
                "enabled": {
                    "type": "boolean"
                },
                "hosts": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "host": {
                                "type": "string"
                            },
                            "paths": {
                                "type": "array",
                                "items": {
                                    "type": "object",
                                    "properties": {
                                        "path": {
                                            "type": "string"
                                        },
                                        "pathType": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "tls": {
                    "type": "array"
                }
            }
        },
        "nameOverride": {
            "type": "string"
        },
        "nodeSelector": {
            "type": "object"
        },
        "podAnnotations": {
            "type": "object"
        },
        "podDisruptionBudget": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "maxUnavailable": {
                    "type": "integer"
                },
                "minAvailable": {
                    "type": "integer"
                }
            }
        },
        "podSecurityContext": {
            "type": "object"
        },
        "replicaCount": {
            "type": "integer"
        },
        "resources": {
            "type": "object"
        },
        "securityContext": {
            "type": "object"
        },
        "service": {
            "type": "object",
            "properties": {
                "ports": {
                    "type": "object",
                    "properties": {
                        "grpc": {
                            "type": "object",
                            "properties": {
                                "port": {
                                    "type": "integer"
                                }
                            }
                        },
                        "http": {
                            "type": "object",
                            "properties": {
                                "port": {
                                    "type": "integer"
                                }
                            }
                        }
                    }
                },
                "type": {
                    "type": "string"
                }
            }
        },
        "serviceAccount": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "create": {
                    "type": "boolean"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "tolerations": {
            "type": "array"
        },
        "topologySpreadConstraints": {
            "type": "array"
        }
    }
}
