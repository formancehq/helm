{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
        "affinity": {
            "type": "object"
        },
        "agent": {
            "type": "object",
            "properties": {
                "authentication": {
                    "type": "object",
                    "properties": {
                        "clientID": {
                            "type": "string"
                        },
                        "clientSecret": {
                            "type": "string"
                        },
                        "existingSecret": {
                            "type": "string"
                        },
                        "issuer": {
                            "type": "string"
                        },
                        "mode": {
                            "type": "string"
                        },
                        "secretKeys": {
                            "type": "object",
                            "properties": {
                                "secret": {
                                    "type": "string"
                                }
                            }
                        },
                        "token": {
                            "type": "string"
                        }
                    }
                },
                "baseUrl": {
                    "type": "string"
                },
                "id": {
                    "type": "string"
                },
                "outdated": {
                    "type": "boolean"
                },
                "production": {
                    "type": "boolean"
                }
            }
        },
        "config": {
            "type": "object",
            "properties": {
                "monitoring": {
                    "type": "object",
                    "properties": {
                        "serviceName": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "debug": {
            "type": "boolean"
        },
        "fullnameOverride": {
            "type": "string"
        },
        "global": {
            "type": "object",
            "properties": {
                "debug": {
                    "type": "boolean"
                },
                "monitoring": {
                    "type": "object",
                    "properties": {
                        "logs": {
                            "type": "object",
                            "properties": {
                                "enabled": {
                                    "type": "boolean"
                                },
                                "format": {
                                    "type": "string"
                                },
                                "level": {
                                    "type": "string"
                                }
                            }
                        },
                        "traces": {
                            "type": "object",
                            "properties": {
                                "batch": {
                                    "type": "boolean"
                                },
                                "enabled": {
                                    "type": "boolean"
                                },
                                "endpoint": {
                                    "type": "string"
                                },
                                "exporter": {
                                    "type": "string"
                                },
                                "insecure": {
                                    "type": "boolean"
                                },
                                "mode": {
                                    "type": "string"
                                },
                                "port": {
                                    "type": "integer"
                                }
                            }
                        }
                    }
                },
                "serviceName": {
                    "type": "string"
                }
            }
        },
        "image": {
            "type": "object",
            "properties": {
                "pullPolicy": {
                    "type": "string"
                },
                "repository": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "imagePullSecrets": {
            "type": "array"
        },
        "nameOverride": {
            "type": "string"
        },
        "nodeSelector": {
            "type": "object"
        },
        "podAnnotations": {
            "type": "object"
        },
        "podSecurityContext": {
            "type": "object"
        },
        "resources": {
            "type": "object",
            "properties": {
                "limits": {
                    "type": "object",
                    "properties": {
                        "cpu": {
                            "type": "string"
                        },
                        "memory": {
                            "type": "string"
                        }
                    }
                },
                "requests": {
                    "type": "object",
                    "properties": {
                        "cpu": {
                            "type": "string"
                        },
                        "memory": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "securityContext": {
            "type": "object"
        },
        "server": {
            "type": "object",
            "properties": {
                "address": {
                    "type": "string"
                },
                "tls": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "insecureSkipVerify": {
                            "type": "boolean"
                        }
                    }
                }
            }
        },
        "serviceAccount": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                }
            }
        },
        "tolerations": {
            "type": "array"
        }
    }
}
